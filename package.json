{
  "name": "trellis",
  "version": "3.2.0-alpha.27",
  "repository": {
    "type": "git",
    "url": "https://github.com/human-nature-lab/trellis-app"
  },
  "description": "The form view component of the Trellis platform.",
  "author": "wyattis <wyatt.israel@yale.edu>, disperse <mark.mcknight@yale.edu>",
  "private": true,
  "scripts": {
    "vite": "npm run compile && vite --host 0.0.0.0",
    "compile": "node compile-translations.js",
    "preview:vite": "npm run compile && vite preview",
    "build:vite": "npm run compile && vite build --mode development",
    "build:mobile": "cross-env APP_ENV=CORDOVA NODE_ENV=production npm run mobile",
    "build:web": "cross-env NODE_ENV=production npm run web",
    "build:dev:android": "npm run build:vite && cordova run android",
    "build:dev:web": "npm run build:vite",
    "release:vite": "npm run compile && vite build --mode production",
    "release:android": "cross-env APP_ENV=CORDOVA NODE_ENV=production npm run release:vite && cordova build android",
    "release:run:android": "cross-env APP_ENV=CORDOVA NODE_ENV=production npm run release:vite && cordova run android",
    "release:prod:android": "cross-env APP_ENV=CORDOVA NODE_ENV=production npm run release:vite && cordova build android --release",
    "check:translations": "node check-translations.js",
    "web": "npm run compile && webpack --config=build/webpack.web.conf.js",
    "mobile": "npm run build:vite",
    "format": "npx eslint --fix \"src/**\"",
    "webpack:android": "npm run compile & webpack --config=build/webpack.mobile.conf.js",
    "webpack:run:android": "npm run webpack:android && cordova run android",
    "webpack:build:android": "npm run webpack:android && cordova build android",
    "webpack:release:android": "cross-env APP_ENV=CORDOVA NODE_ENV=production npm run webpack:build:android",
    "start": "npm run compile && webpack-dev-server --config=build/webpack.mobile.conf.js",
    "start:web": "npm run compile && webpack-dev-server --config=build/webpack.web.conf.js"
  },
